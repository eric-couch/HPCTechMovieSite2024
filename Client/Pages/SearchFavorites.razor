@page "/searchfavorites"

<h3>SearchFavorites</h3>

<div class="container-fluid mb-5">
    <div class="row">
        <div class="col-6">
            <input type="text" class="form-control" @bind="searchTerm" />
        </div>
        <div class="col-1">
            <button class="btn btn-primary" @onclick="Search">Search</button>
        </div>
    </div>
</div>

@* output caching *@
<p>results: @((MarkupString)searchResult)</p>