{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=HPCTechMovieSite;Trusted_Connection=True"
  },
  "Logging": {
    "LogLevel": { // All providers, LogLevel applies to all the enabled providers
      "Default": "Information", // Default loggin, Information and higher.
      "Microsoft.AspNetCore": "Warning" // all Microsoft.AspNetCore namespace, Warning and higher
    },
    "Debug": { // Debug provider
      "LogLevel": {
        "Default": "Information" // Overrides the preceding
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      }
    }

  },
  "IdentityServer": {
    "Clients": {
      "HPCTechMovieSite2024.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}

// Log Levels
// 0 = Trace, 1 = Debug, 2 = Information, 3 = Warning, 4 = Error, 5 = Critical, 6 = None

